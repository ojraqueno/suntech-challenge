{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information"
    }
  },
  "extensions": {
    "queues": {
      "visibilityTimeout": "00:00:10",
      "maxPollingInterval": "00:00:15",
      "maxDequeueCount": 3
    }
  }
}